@page
@model RosBilRP.Pages.Biler.OpretModel
@{
}

<h1 class="display-3 mb-5">Opret ny Bil</h1>

<form method="post">
    <div class="form-group mb-3">
        <label asp-for="@Model.Element.Nummerplade" class="control-label"></label>
        <input asp-for="@Model.Element.Nummerplade" class="form-control" />
        <span asp-validation-for="@Model.Element.Nummerplade" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label asp-for="@Model.Element.Model" class="control-label"></label>
        <input asp-for="@Model.Element.Model" class="form-control" />
        <span asp-validation-for="@Model.Element.Model" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label asp-for="@Model.Element.PrisPrDag" class="control-label"></label>
        <input asp-for="@Model.Element.PrisPrDag" class="form-control" />
        <span asp-validation-for="@Model.Element.PrisPrDag" class="text-danger"></span>
    </div>

    <p>
        <div class="form-group mb-3">
            <input type="submit" value="Opret" class="btn btn-outline-primary" />
        </div>
    </p>
</form>
