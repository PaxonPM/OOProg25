@page
@model RosBilRP.Pages.Biler.HistorikModel
@{
}

<h1 class="display-3 mb-5">Lejehistorik for @Model.Bilen.NummerpladeOgModel</h1>

<table class="table table-bordered table-hover table-striped">
	<thead>
		<tr>
			<th>
				Id
			</th>
			<th>
				Dato
			</th>
			<th>
				Antal Dage
			</th>
		</tr>
	</thead>
	<tbody>
		@if (Model.Data != null)
		{
			foreach (var leje in Model.Data)
			{
				<tr>
					<td>
						@leje.Id
					</td>
					<td>
						@leje.Dato.ToShortDateString()
					</td>
					<td>
						@leje.AntalDage
					</td>
				</tr>
			}
		}
	</tbody>
</table>
